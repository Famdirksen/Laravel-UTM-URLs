{
    "name": "famdirksen/laravel-utm-urls",
    "type": "library",
    "description": "This package will add utm attributes to all urls",
    "keywords": [
        "Famdirksen",
        "LaravelUTMUrls"
    ],
    "homepage": "https://github.com/Famdirksen/LaravelUTMUrls",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Dirksen",
            "email": "robin@famdirksen.nl",
            "homepage": "https://www.famdirksen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.1",
        "php" : "~5.6|~7.0|~7.1|~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Famdirksen\\LaravelUTMUrls\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famdirksen\\LaravelUTMUrls\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Famdirksen\\LaravelUTMUrls\\LaravelUTMUrlsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
